{
  "name": "AIDatingCoachMobile",
  "version": "1.0.0",
  "private": true,
  "author": "AI Dating Coach Team",
  "description": "AI Dating Coach Mobile App - React Native application for iOS and Android with AI-powered dating insights",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android-bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "cd ios && xcodebuild -workspace AIDatingCoachMobile.xcworkspace -scheme AIDatingCoachMobile -configuration Release -destination generic/platform=iOS -archivePath AIDatingCoachMobile.xcarchive archive",
    "build:ios-release": "./scripts/build-ios-release.sh",
    "build:android-release": "./scripts/build-android-release.sh",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-clipboard/clipboard": "^1.14.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@stripe/stripe-react-native": "^0.38.6",
    "@supabase/supabase-js": "^2.45.4",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-animatable": "^1.4.0",
    "react-native-device-info": "^10.13.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-haptic-feedback": "^2.3.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-keychain": "^8.2.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^4.1.5",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-share": "^10.2.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
